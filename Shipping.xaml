<Window x:Class="Lab_1.Shipping"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_1"
        mc:Ignorable="d"
        Title="Shipping" Height="450" Width="800">
    <Grid>
        <Label x:Name="Weight_Lbl" Content="Weight" HorizontalAlignment="Left" Margin="112,60,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ShippingFee_Lbl" Content="Total shipping Fee" HorizontalAlignment="Left" Margin="112,132,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.953,0.495"/>
        <TextBox x:Name="ShippingFee_TxtBox" HorizontalAlignment="Left" Margin="231,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="TotalFee_TxtBox" HorizontalAlignment="Left" Margin="231,136,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="FeeCalc_Btn" Content="Calculate" HorizontalAlignment="Left" Margin="150,217,0,0" VerticalAlignment="Top" Click="FeeCalc_Btn_Click"/>
        <Button x:Name="return_Btn" Content="Main Page" HorizontalAlignment="Left" Margin="291,217,0,0" VerticalAlignment="Top" Width="60" Click="return_Btn_Click"/>

    </Grid>
</Window>
