<Window x:Class="Lab_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <CheckBox x:Name="BankCheckBox" Content="Bank Charges Calculator" HorizontalAlignment="Left" Margin="228,80,0,0" VerticalAlignment="Top" Checked="BankCheckBox_Checked"/>
        <CheckBox x:Name="ShippingCheckBox" Content="Shipping Calculator" HorizontalAlignment="Left" Margin="228,126,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.123,0.213" Checked="ShippingCheckBox_Checked" IsChecked="False"/>
        <CheckBox x:Name="PopulationCheckBox" Content="Population Calculator" HorizontalAlignment="Left" Margin="228,171,0,0" VerticalAlignment="Top"/>
        <Button x:Name="GoToBtn" Content="Select Page" HorizontalAlignment="Left" Margin="302,269,0,0" VerticalAlignment="Top" Width="98" Click="GoToBtn_Click"/>

    </Grid>
</Window>
